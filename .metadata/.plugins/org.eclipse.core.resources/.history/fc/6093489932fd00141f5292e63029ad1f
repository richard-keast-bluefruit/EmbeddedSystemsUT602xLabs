#include <stdint.h>
#include "tm4c123gh6pm.h"
#include "LaunchPad.h"

//*****************************************************************************
//
//! \addtogroup example_list
//! <h1>Blinky (blinky)</h1>
//!
//! A very simple example that blinks the on-board LED using direct register
//! access.
//
//*****************************************************************************

//*****************************************************************************
//
// Blink the on-board LED.
//
//*****************************************************************************
int
main(void)
{
   LaunchPad launchPad;

   while(1)
   {
      launchPad.Run();
   }
}
